{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "OPENAI_API_KEY": "@openai_api_key",
        "OPENAI_AGENT_ID": "@openai_agent_id",
        "ENVIRONMENT": "production"
    }
}